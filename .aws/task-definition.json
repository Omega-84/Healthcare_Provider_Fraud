{
    "family": "healthcare-fraud-task",
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::310342906701:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "healthcare-fraud",
            "image": "310342906701.dkr.ecr.us-east-1.amazonaws.com/healthcare-fraud:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/healthcare-fraud-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}